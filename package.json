{
  "name": "routes-react-router",
  "displayName": "routes-react-router",
  "description": "Easy way to visualize react-router 7 routes paths",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "typescript",
        "extensions": [
          ".ts"
        ]
      }
    ],
    "codeLensProviders": [
      {
        "language": "typescript",
        "pattern": "**/routes.ts"
      }
    ]
  },
  "scripts": {
    "biome:format": "pnpm biome format --write src",
    "biome:lint": "pnpm biome lint --write src",
    "biome:check": "pnpm biome check --write src",
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run biome:check",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "20.x",
    "@types/vscode": "1.100.0",
    "@vscode/test-cli": "0.0.10",
    "@vscode/test-electron": "2.5.2",
    "typescript": "5.8.3"
  }
}
